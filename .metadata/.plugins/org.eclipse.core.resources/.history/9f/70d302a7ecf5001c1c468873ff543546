<properties>
	
	<comment>회원 관련 쿼리문들</comment>
	<entry key="insert1Pet">
		INSERT INTO ANIMAL
			(PET_NO, 
			 PET_NAME, 
			 PET_INPUTNO,
			 PET_KIND, 
			 PET_BREED, 
			 PET_BIRTH, 
			 PET_NEUTRAL,
			 PET_OWNER)
		VALUES
			(SEQ_ANO.NEXTVAL, 
			 ?, 
			 ?, 
			 ?, 
			 ?,
			 TO_DATE(?,'YYYY-MM-DD'), 
			 ?,
			 ?)
	</entry>
	<entry key="insert2Pet">
		INSERT INTO ATTACHMENT
			(FILE_NO, 
			 REF_CNO, 
			 ORIGIN_NAME, 
			 CHANGE_NAME, 
			 FILE_PATH, 
			 FILE_LEVEL)
		VALUES
			(SEQ_FNO.NEXTVAL, 
			 SEQ_ANO.CURRVAL, 
			 ?, 
			 ?, 
			 ?, 
			 ?)
	</entry>
	<entry key="selectPetList">
		SELECT PET_OWNER, PET_NO, FILE_PATH||CHANGE_NAME TITLEIMG, PET_NAME
		  FROM ANIMAL A
		  JOIN ATTACHMENT ON(PET_NO = REF_CNO)
		 WHERE A.STATUS = 'Y'
		   AND PET_OWNER = ?
		   AND FILE_LEVEL = 1
	</entry>

	<entry key="selectDate">
	SELECT *
	FROM DIARY
	WHERE WRITE_DATE = ?
	  AND PET_NO = ?
	</entry>
</properties>